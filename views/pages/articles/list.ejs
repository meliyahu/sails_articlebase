<h1 class="display-4">Articles</h1>
<table class="table table-striped">
   <thead>
    <tr>
        <th>#</th>
        <th>Title</th>
        <th>Body</th>
    </tr>
   </thead>
   <tbody>
       <% articles.forEach((article) => { %>
         <tr>
             <td><%= article.id %></td>
             <td><%= article.title %></td>
             <td><%= article.body %></td>
         </tr>
       <% }) %>
   </tbody>
</table>